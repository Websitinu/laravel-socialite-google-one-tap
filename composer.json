{
    "name": "websitinu/laravel-socialite-google-one-tap",
    "description": "A very light package to install Google One Tap Login on your Laravel web application that you have already installed Socialite",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "google one tap",
        "laravel",
        "websitinu",
        "socialite",
        "login with google",
        "box register google in top"
        
    ],
    "autoload": {
        "psr-4": {
            "LaravelSocialite\\GoogleOneTap\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Ehsan Ghafoori",
            "email": "ehsan@websitinu.com"
        }
    ],

    "require": {
        "php": "^8.2",
        "ext-json": "*",
        "google/apiclient": "^2.18",
        "socialiteproviders/manager": "^4.8"
    },
    "require-dev": {
        "orchestra/testbench": "^9.12",
        "pestphp/pest": "^2.36",
        "phpunit/phpunit": "10.5.36"
    },

    "autoload-dev": {
        "psr-4": {
            "SocialiteProviders\\GoogleOneTap\\Tests\\": "tests"
        }
    },
    "scripts": {
        "google-task-composer-cleanup": "Google\\Task\\Composer::cleanup",
        "pest": "./vendor/bin/pest"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "google/apiclient-services": [
            "Oauth2"
        ]
    }
}
